# Discord Bot Configuration
# Copy this file to .env and fill in your actual values

# Bot Token - Get this from Discord Developer Portal
DISCORD_TOKEN=your_bot_token_here

# Bot Prefix for commands
BOT_PREFIX=$

# Test Bot Configuration (optional)
# Uncomment these if you have a test bot
# TEST_DISCORD_TOKEN=your_test_bot_token_here
# TEST_BOT_PREFIX=$

# Bot Type - either 'DATA' or 'DATAtestsim'
BOT_TYPE=DATA

# Database Configuration
DATABASE_PATH=./utils/stfc_bot.db

# Logging Configuration
LOG_LEVEL=INFO

# Discord Bot Settings
DISCORD_INTENTS_MESSAGE_CONTENT=true
DISCORD_INTENTS_GUILDS=true
DISCORD_INTENTS_MEMBERS=true